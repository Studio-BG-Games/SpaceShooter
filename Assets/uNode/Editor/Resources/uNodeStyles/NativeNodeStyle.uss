.compact-icon {
    width: 30px;
    height: 30px;
    margin-bottom: 5px;
    margin-right: 1px;
    flex-shrink: 0;
    align-self: center;
}

#title > .compact-input {
    padding-right: 0;
    padding-left: 2px;
    align-self: center;
}

#title > .compact-input > Image {
    display: none;
    visibility: hidden;
}

#title > .compact-input > #type {
    display: none;
    visibility: hidden;
}

#title > .compact-input > #connector {
    margin-right: 2px;
}

#title > .compact-output {
    padding-left: 0;
    padding-right: 2px;
    align-self: center;
    flex-shrink: 0;
}

#title > .compact-output > Image {
    width: 24px;
    height: 24px;
    display: none;
    visibility: hidden;
}

#title > .compact-output > #type {
    display: none;
    visibility: hidden;
}

#title > .compact-output > #connector {
    margin-left: 2px;
}

.compact-node > #node-border > #title > #title-icon {
    margin-left: 0;
}

.compact-node > #node-border > #title > #title-label {
    margin-left: 0;
    margin-right: 0;
}

.compact-node > #node-border > #title > .compact-control {
    align-self: center;
}

#title.rich-title > Label {
    display: none;
    visibility: hidden;
}

/* block node */

#node-blocks.compact > #node-border {
    background-color:rgba(29,29,29,0.8);
	margin-top: 2px;
	margin-left: 2px;
	margin-right: 2px;
	margin-bottom: 2px;
}

#node-blocks.compact > #node-border > #block-container
{
	margin-bottom: 5px;
}

#node-blocks.compact > #node-border > #title
{
    background-color:rgba(0,0,0,0);
	border-radius:2px;
	margin-left: 3px;
	padding-right: 22px;
	height: 25px;
    justify-content:flex-start;
    flex-direction:row;
}

#node-blocks.compact > #node-border > #title > #title-icon
{
    position:Absolute;
	visibility: hidden;
}

#node-blocks > #node-border > #title
{
    background-color:rgba(0,0,0,0);
	border-radius:2px;
	height: 25px;
    justify-content:center;
    align-items:center;
    flex-direction:row;
}

#node-blocks > #node-border > #title > #title-label
{
    flex-basis:auto;
    flex-grow: 0;
	margin-top: 3px;
	margin-left: 0;
	margin-right: 3px;
	margin-bottom: 3px;
	font-size: 14px;
}

#node-blocks > #node-border > #title > #title-icon
{
	width: 20px;
	height: 20px;
}

#node-blocks > #node-border > #contents > #divider
{
    background-color:rgba(0,0,0,0);
    border-color:rgba(0,0,0,0);
}

#node-blocks > #node-border > #inside > #contents
{
    margin-top : 0;
}

#node-blocks > #node-border.empty > #inside > #contents
{
    margin-top : 0;
    padding-top : 0;
    margin-bottom : 0;
    padding-bottom : 0;
}

#block-container > #hint
{
    border-left-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-top-width: 1px;
    border-color: #393939;
    text-color: #393939;
    height: 24px;
    text-alignment: middle-center;
    border-radius:8px;
	margin: 2px;
}


#node-blocks.transition > #node-border > #block-container > #hint
{
    border-color: #127600;
    background-color: #074400;
    text-color: #a2a2a2;
}

.dragdisplay
{
    position:absolute;
    left:0;
    right: 0;
    height:4px;
    margin-top:-2px;
    background-color: rgb(210, 152, 0);
}

/* Transitions */
.transition #selection-border
{
	border-radius: 12px;
}

.transition > #node-border
{
    background-color:rgba(10,55,25,0.8);
    border-color:rgba(23,171,106,0.8);
	border-radius: 10px;
    border-left-width:1px;
    border-right-width:1px;
    border-top-width:1px;
    border-bottom-width:1px;
    padding-top : 5px;
    padding-bottom : 5px;
    margin-top : 2px;
    margin-bottom : 2px;
    margin-left:2px;
    margin-right:2px;
}

.transition > #node-border > #title
{
    background-color:rgba(29,29,29,0);
    justify-content:flex-start;
	height: 18px;
}

.transition > #node-border > #title > #title-label
{
    flex:1 0 auto;
	margin-top: 3px;
	margin-left: 3px;
	margin-right: 3px;
	margin-bottom: 3px;
}

.transition > #node-border > #contents > #divider
{
    background-color:rgba(29,29,29,0);
    border-color:rgba(29,29,29,0);
}

.transition > #node-border > #inside > #contents
{
    margin-top : 0;
}

.transition > #node-border.empty > #inside > #contents
{
    margin-top : 0;
    padding-top : 0;
    margin-bottom : 0;
    padding-bottom : 0;
}

.transition #portInputContainer {
    position: absolute;
    width: 412px;
    left: -400px;
    top: 46px;
}